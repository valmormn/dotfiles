liases

alias c="clear"
alias pip="pip3"
alias python="python3"

alias add='git add'
alias push='git push'
alias pull='git pull'
alias commit='git commit -am'
alias clone='git clone'
alias gitall='git add . && git commit -am'
alias pushnow='npm run push'
alias status='git status'

alias cache="git config --global credential.helper \'cache --timout=604800\'"

alias upgd='sudo apt-get update && sudo apt-get upgrade'
alias get='sudo apt-get install'

alias say='echo coe mane!!!!'

alias v='vi'
alias nv='nvim'


alias shconfig='source ~/.basrc'

